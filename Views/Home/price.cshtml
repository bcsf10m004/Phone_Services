    <script type="text/javascript">

        $(document).ready(function () {



            $('#b1').click(function () {

                var price1 = $('#p1').val();
                var price2 = $('#p2').val();

                alert('p1 =' + price1);
                $("#t1").children().remove();
                $.getJSON("/Home/SearchByprice?p1=" + price1,"p2="+ price2, function (data) {

                    alert('Appending' + data);
                    if (data) {
                        $("#t1").append('<tr><td><h2>Company</h2></td><td><h2>Model</h2></td><td><h2>Price</h2></td><td><h2>Rating</h2></td><td><h2>Comments</h2></td><td><h2>Availability</h2></td><td><h2>Features</h2></td><td><h2>Images</h2></td></tr>');
                        $.each(data, function (i, item) {

                            $("#t1").append('<tr><td><strong>' + item.Company + '</strong></td><td><strong>' + item.Model + '</strong></td><td><strong>' + item.Price + '</strong></td><td><strong>' + item.Rating + '</strong></td><td><strong>' + item.Comments + '</strong></td><td><strong>' + item.Availability + '</strong></td><td><strong>' + item.Features + '</strong></td><td><img width="120" height="150" class="image_frame" src="' + item.Image + '"></td></tr>');
                        });
                    }
                    else {
                        $("#t1").append("<tr><td><h2>Model Not Found</h2></tr></td>");
                    }

                });

            });

        });

        </script>



